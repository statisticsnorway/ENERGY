* Encoding: UTF-8.
GET FILE='data\hhq.sav' .
VARSTOCASES
  /MAKE AB1 FROM AB1$01 TO AB1$20
  /MAKE AB3 FROM AB3$01 TO AB3$20
  /MAKE AB3B FROM AB3B$01 TO AB3B$20
  /MAKE AB4 FROM AB4$01 TO AB4$20
  /MAKE AB5 FROM AB5$01 TO AB5$20
  /MAKE AB6 FROM AB6$01 TO AB6$20
  /MAKE AC1 FROM AC1$01 TO AC1$20
  /MAKE AC2 FROM AC2$01 TO AC2$20
  /MAKE AC3 FROM AC3$01 TO AC3$20
  /MAKE AC4 FROM AC4$01 TO AC4$20
  /MAKE AC5 FROM AC5$01 TO AC5$20
  /MAKE AC6 FROM AC6$01 TO AC6$20
  /MAKE AC7 FROM AC7$01 TO AC7$20
  /MAKE AC8 FROM AC8$01 TO AC8$20
  /MAKE AC9 FROM AC9$01 TO AC9$20
  /MAKE A2 FROM A2$01 TO A2$20
  /MAKE A3 FROM A3$01 TO A3$20
  /MAKE A4 FROM A4$01 TO A4$20
  /MAKE A5 FROM A5$01 TO A5$20
  /MAKE A6 FROM A6$01 TO A6$20
  /MAKE A7 FROM A7$01 TO A7$20
  /MAKE A8 FROM A8$01 TO A8$20
  /INDEX=Person_no(20) 
  /KEEP=AA1 AA2 AA3 AA4 AA5 AA6 AA8 AALOGIN AA7 AA7A AA7B AA9 AA10
  /NULL=DROP.

SAVE OUTFILE='data\person_test_tz.sav' .

GET DATA  /TYPE=TXT
  /FILE="hhq\hhq_test_tz.sps"
  /DELIMITERS=" "
  /FIRSTCASE=1
  /MULTIPLESPACES IGNORE=YES
  /VARIABLES=
  Varname AUTO
  Varpos AUTO
  /MAP.

SELECT IF (char.index(varname,'$') > 0).
SELECT IF not(char.substr(varname,2,1) ='/').
EXECUTE.
STRING command (a17).
STRING dot (a1).
COMPUTE command= 'DELETE VARIABLES'.
COMPUTE dot = '.'.
EXECUTE.
WRITE OUTFILE='temp\delvar.txt'
 /1 command varname dot.
EXECUTE.

GET FILE='data\hhq_test_tz.sav' .
INSERT FILE ='temp\delvar.txt'.
EXECUTE.

SAVE OUTFILE='data\household_test_tz.sav' .





